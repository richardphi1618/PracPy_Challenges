[flake8]
# ignore = E501, W503
# # E501: line too long
# # W503: line break before binary operator
max-line-length = 90
exclude =
    # No need to traverse our git directory
    .git,
    # There's no value in checking cache directories
    __pycache__,
    # .venv folder has external python packages that dont pass...
    .venv
